{
  "build": {
    "env": {
      "MICROCMS_API_KEY": "@tikuwabar-cms-apikey",
      "GA_MEASUREMENT_ID": "@tikuwabar-ga-id"
    }
  },
  "alias": "tikuwabar.shojir.ooo",
  "routes": [
    {"src": "/", "headers": {"cache-control": "max-age=86400, s-maxage=21600, public"}},
    {"src": "/privacy-policy", "headers": {"cache-control": "max-age=604800, s-maxage=86400, public"}},
    {"src": "/news", "headers": {"cache-control": "max-age=86400, s-maxage=21600, public"}},
    {"src": "/news/.*", "headers": {"cache-control": "max-age=604800, s-maxage=86400, public"}}
  ]
}
